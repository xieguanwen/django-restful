<uwsgi>
<socket>127.0.0.1:7001</socket>
<uid>daemon</uid>
<gid>daemon</gid>
<listen>80</listen>
<master>true</master>
<pidfile>/var/run/postStation.pid</pidfile>
<processes>4</processes>
<workers>4</workers>
<pcre-jit>true</pcre-jit>
<module>xljApi.wsgi</module>
<pythonpath>/data/www/xljApi</pythonpath>
<env>PYTHON_EGG_CACHE=/tmp/.python-eggs</env>
<profiler>true</profiler>
<memory-report>true</memory-report>
<enable-threads>true</enable-threads>
<logdate>true</logdate>
<daemonize>/data/www/xljApi/logs/postStation.log</daemonize>
</uwsgi>